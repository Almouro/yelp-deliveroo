{
  "manifest_version": 2,

  "name": "Yelp Deliveroo",
  "description": "See Yelp info for restaurants on Deliveroo.com",
  "version": "1.0",

  "browser_action": {
    "default_icon": "hamburger.png"
  },
  "icons": {
    "128": "hamburger.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "all_frames": true,
    "js": [ "index.js" ],
    "matches": [ "https://deliveroo.fr/*/*" ],
    "run_at": "document_end"
  }]
}
