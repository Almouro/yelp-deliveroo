{
  "manifest_version": 2,

  "name": "Yelp Deliveroo",
  "description": "See Yelp info for restaurants on Deliveroo",
  "version": "1.4",

  "browser_action": {
    "default_icon": "hamburger.png"
  },
  "icons": {
    "128": "hamburger.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "all_frames": true,
    "js": [ "dev.js" ],
    "matches": [
      "https://deliveroo.fr/*/*",
      "https://deliveroo.co.uk/*/*"
    ],
    "run_at": "document_end"
  }]
}
